{
	"name": "authwave/php-client",
	"description": "PHP client library to implement Authwave in your application",

	"require": {
		"php": ">=7.4",
		"ext-openssl": "*",
		"phpgt/http": "1.*",
		"phpgt/session": ">=1.1"
	},
	"require-dev": {
		"phpunit/phpunit": "8.*"
	},
	"autoload": {
		"psr-4": {
			"Authwave\\": "./src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Authwave\\Test\\": "./test/phpunit"
		}
	}
}